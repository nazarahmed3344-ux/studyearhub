    <script>
        // Currency Conversion Rates
        const exchangeRates = {
            'PKR': { symbol: '₨', rate: 1 },
            'USD': { symbol: '$', rate: 280 },
            'EUR': { symbol: '€', rate: 300 },
            'AED': { symbol: 'AED', rate: 76 },
            'SAR': { symbol: 'SR', rate: 75 }
        };

        // Income amounts in PKR
        const incomeAmounts = [
            { min: 5000, max: 50000 }, // Affiliate
            { min: 10000, max: 100000 }, // Reselling
            { min: 8000, max: 80000 } // Content
        ];

        // Current date
        document.getElementById('currentDate').textContent = new Date().toLocaleDateString();

        // Currency conversion function
        function changeCurrency(cardIndex, currency) {
            const incomeElement = document.getElementById('income' + (cardIndex + 1));
            const buttons = document.querySelectorAll('.currency-options')[cardIndex].querySelectorAll('.currency-btn');
            
            // Update active button
            buttons.forEach(btn => btn.classList.remove('active'));
            buttons.forEach(btn => {
                if (btn.textContent === currency) btn.classList.add('active');
            });
            
            // Convert amount
            const rate = exchangeRates[currency].rate;
            const symbol = exchangeRates[currency].symbol;
            const minAmount = Math.round(incomeAmounts[cardIndex].min / rate);
            const maxAmount = Math.round(incomeAmounts[cardIndex].max / rate);
            
            incomeElement.textContent = `${symbol}${minAmount} - ${symbol}${maxAmount}/month`;
        }

        // Business functions
        function showWelcome() {
            alert('🎓 Welcome to StudyGearHub Business Platform!\nStart earning today with multiple income streams.');
