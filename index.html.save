                alert(`✅ Registration Successful!\n\nName: ${name}\nEmail: ${email}\nPhone: ${phone}\nCountry: ${country}\nBusiness: ${businessType}\n\nWe will contact you within 24 hours.`);
                
                // Reset form
                document.getElementById('fullName').value = '';
                document.getElementById('email').value = '';
                document.getElementById('phone').value = '';
            } else {
                alert('❌ Please fill all required fields.');
            }
        }

        function emailSupport() {
            alert('📧 Email: nazarahmed3344@gmail.com\nWe respond within 24 hours.');
        }

        function emergencySupport() {
